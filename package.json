{
  "name": "tskc",
  "version": "0.0.0",
  "private": true,
  "description": "JHipster Project",
  "license": "UNLICENSED",
  "scripts": {
    "eslint": "eslint './**/*.{ts,js}' --fix",
    "eslint:ci": "eslint './**/*.{ts,js}'",
    "prepare": "husky install",
    "prettier:check": "prettier --check \"{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}\"",
    "prettier:format": "prettier --write \"{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watch:all": "jest --watchAll"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "1.2.0",
    "@types/jest": "27.4.1",
    "@typescript-eslint/eslint-plugin": "5.15.0",
    "@typescript-eslint/parser": "5.15.0",
    "eslint": "8.11.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.6",
    "prettier": "2.6.0",
    "prettier-plugin-java": "1.6.1",
    "prettier-plugin-packagejson": "2.2.16",
    "ts-jest": "27.1.3",
    "typescript": "4.6.2"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "cacheDirectories": [
    "node_modules"
  ]
}
